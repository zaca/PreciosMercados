<?xml version='1.0' encoding='utf-8'?>
<widget id="com.madmax.preciosMercados" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Compass">
        <param name="android-package" value="org.apache.cordova.deviceorientation.CompassListener" />
    </feature>
    <name>PreciosMercados</name>
    <description>
		Busqueda de precios en distintos mercados centrales del pais.
	</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
		Zacarias www.itArchitecturalNotes.com
	</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="LogLevel" value="DEBUG" />
</widget>
